filter {

  if [type] in ["errorlogs", "ee2errorlogs"] {
    fingerprint {
        target => "fingerprint"
        method => "SHA256"
        source => ["@timestamp","user"]
        key => "0keyd0key"
        base64encode => true
        concatenate_sources => true
    } else {
      fingerprint {
          target => "fingerprint"
          method => "UUID"
      }
    }
  }
}